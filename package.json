{
  "name": "solana-rpc-client-generator",
  "version": "1.0.0",
  "description": "Generate type-safe Solana RPC clients in 10 languages from OpenRPC spec",
  "type": "module",
  "main": "index.js",
  "bin": {
    "solana-rpc-gen": "./bin/cli.ts"
  },
  "scripts": {
    "generate": "npm run generate:ts && npm run generate:go && npm run generate:python && npm run generate:java && npm run generate:csharp && npm run generate:rust && npm run generate:php && npm run generate:ruby && npm run generate:swift && npm run generate:kotlin",
    "generate:ts": "ts-node --esm scripts/generate-client.ts",
    "generate:go": "ts-node --esm scripts/generate-go.ts",
    "generate:python": "ts-node --esm scripts/generate-python.ts",
    "generate:java": "ts-node --esm scripts/generate-java.ts",
    "generate:csharp": "ts-node --esm scripts/generate-csharp.ts",
    "generate:rust": "ts-node --esm scripts/generate-rust.ts",
    "generate:php": "ts-node --esm scripts/generate-php.ts",
    "generate:ruby": "ts-node --esm scripts/generate-ruby.ts",
    "generate:swift": "ts-node --esm scripts/generate-swift.ts",
    "generate:kotlin": "ts-node --esm scripts/generate-kotlin.ts",
    "generate:openrpc": "open-rpc-generator generate -c open-rpc-generator-config.json",
    "fetch-docs": "ts-node --esm scripts/fetch-docs.ts",
    "build": "npm run generate:ts && cd generated/typescript && tsc",
    "test": "jest --passWithNoTests",
    "test:ts": "jest tests/typescript/",
    "test:py": "cd generated/python && python -m pytest ../../tests/python/ -v",
    "test:quick": "ts-node --esm scripts/test-client.ts"
  },
  "keywords": [
    "solana",
    "rpc",
    "openrpc",
    "client",
    "generator"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@open-rpc/generator": "^2.1.1",
    "@open-rpc/meta-schema": "^1.14.9",
    "@open-rpc/schema-utils-js": "^2.1.2",
    "@types/jest": "^29.5.14",
    "jest": "^29.7.0",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
